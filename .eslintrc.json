{
  "extends": "airbnb",
  "plugins": [
      "react",
      "react-native"
  ],
  "ecmaFeatures": {
      "classes": true,
      "jsx": true
  },
  "rules": {
    "react/prop-types": 1,
    "react/prefer-es6-class": [0, "always"],
	"no-unused-vars": "warn",
	"eqeqeq": ["error", "smart"]
  },
  "env": {
	  "node": true,
	  "jest": true
  }
}
